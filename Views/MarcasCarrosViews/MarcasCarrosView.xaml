<UserControl x:Class="ProyectoBD.Views.MarcasCarrosViews.MarcasCarrosView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProyectoBD.Views.MarcasCarrosViews"
             xmlns:vm="clr-namespace:ProyectoBD.ViewModels.MarcasCarrosViewModels"
             mc:Ignorable="d"
             d:DataContext="{d:DesignInstance Type=vm:MarcasCarrosViewModel}"
             Visibility="{Binding ControlVisibility}"
             d:DesignHeight="600" d:DesignWidth="1200">
    <Grid>
        <local:CrearMarcasCarrosView DataContext="{Binding CrearMarcasCarrosViewModel}"/>
        <local:VerMarcasCarrosView DataContext="{Binding VerMarcasCarrosViewModel}"/>
    </Grid>
</UserControl>
