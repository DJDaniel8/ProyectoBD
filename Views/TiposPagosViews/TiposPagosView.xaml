<UserControl x:Class="ProyectoBD.Views.TiposPagosViews.TiposPagosView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProyectoBD.Views.TiposPagosViews"
             xmlns:vm="clr-namespace:ProyectoBD.ViewModels.TiposPagosViewModels"
             mc:Ignorable="d" 
             d:DataContext="{d:DesignInstance Type=vm:TiposPagosViewModel}"
             Visibility="{Binding ControlVisibility}"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <local:CrearTiposPagosView DataContext="{Binding CrearTiposPagosViewModel}"/>
        <local:VerTiposPagosView DataContext="{Binding VerTiposPagosViewModel}"/>
    </Grid>
</UserControl>
