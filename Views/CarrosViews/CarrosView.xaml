<UserControl x:Class="ProyectoBD.Views.CarrosViews.CarrosView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProyectoBD.Views.CarrosViews"
             xmlns:vm="clr-namespace:ProyectoBD.ViewModels.CarrosViewModels"
             mc:Ignorable="d" 
             d:DataContext="{d:DesignInstance Type=vm:CarrosViewModel}"
             Visibility="{Binding ControlVisibility}"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <local:CrearCarrosView DataContext="{Binding CrearCarrosViewModel}"/>
        <local:VerCarrosView DataContext="{Binding VerCarrosViewModel}"/>
    </Grid>
</UserControl>
